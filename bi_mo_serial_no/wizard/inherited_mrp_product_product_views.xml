<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_res_config_mo_inherit_wizard" model="ir.ui.view">
		<field name="name">mrp.product.produce.inherite</field>
		<field name="model">mrp.product.produce</field>
		<field name="inherit_id" ref="mrp.view_mrp_product_produce_wizard"/>

		<field name="arch" type="xml">
		<xpath expr="//field[@name='lot_id']" position="attributes">
                    <attribute name="attrs">{'required': False}</attribute>
                </xpath>
		 <xpath expr="//button[@name='do_produce']" position="after">
			<button name="do_produce_more" type="object" string="Produce Next" class="btn-primary"/>
		</xpath>
			</field>
		</record>
</odoo>
